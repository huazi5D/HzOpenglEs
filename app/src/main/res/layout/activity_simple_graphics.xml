<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center" >

    <Button
        android:id="@+id/triangle"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="@string/simple_button_sg" />

    <Button
        android:id="@+id/square"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/triangle"
        android:text="@string/simple_button_tg" />

    <Button
        android:id="@+id/circul"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/square"
        android:text="@string/simple_button_cr" />

    <Button
        android:id="@+id/cube"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/circul"
        android:text="@string/simple_button_cb" />

    <Button
        android:id="@+id/cone"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/cube"
        android:text="@string/simple_button_cn" />

    <Button
        android:id="@+id/cylinder"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/cone"
        android:text="@string/simple_button_cl" />

    <Button
        android:id="@+id/sphere"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_below="@+id/cylinder"
        android:text="@string/simple_button_sp" />

</RelativeLayout>
